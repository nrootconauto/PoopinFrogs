activate->CFrogNum |
  activate_other=CFrogBlock
  world=C3DWorldManager
|
  world _ #world .
  
  activated_at < 0. ifTrue: [
    activated_at _ CFrogTime now asSeconds .
    world->doors do:[:d=CDoor|
      ((d->x - x) abs = 1 or: (d->y - y) abs = 1) ifTrue: [
        d->activated_at < 0 ifTrue: [
          d activate
        ] .
      ] .
    ] .
    ^ 1 .
  ].
   ^ 0 .
