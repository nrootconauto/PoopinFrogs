init | |
  #world addThing: self .
  speed _ .5 / 30 * 2 .
  health _ 40 .
  random_generator _ CFrogRandom new .
  template _ #world getThingTemplate: 'CZombie' .
  animation_start_tS _ 0 .
  state _ 'Normal' . 
  piss_stream _ CPissStream new .
  no_fall_off_edge _ 1 .
  piss_stream ->from _ self .
  ^ self
