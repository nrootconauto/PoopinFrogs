angleBetween: other |
  px=CFrogNum
  py=CFrogNum
  ox=CFrogNum
  oy=CFrogNum
  r=CFrogNum
|
  ox _ other xPos - x .
  oy _ other yPos - y .
  px _ angle cos * 100 .
  py _ angle sin * 100 .
  r _ (
      ((px * ox) + (py * oy ) ) / 
       (((( px * px ) + ( py * py )) sqrt 
         * (( ox * ox )+ ( oy * oy )) sqrt ) + .1 )
    ) arccos .
  ^ r